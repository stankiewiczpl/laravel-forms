{
    "name": "stankiewiczpl/laravel-forms",
    "description": "Laravel exntended forms",
    "keywords": [
        "laravel",
        "laravel forms",
        "laravel custom fields"
    ],
    "homepage": "https://github.com/stankiewiczpl/laravel-forms",
    "authors": [
        {
            "name": "Piotr Stankiewicz",
            "email": "stankiewicz.opole@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "laravelcollective/html": "^6.0",
        "kris/laravel-form-builder": "^1.30",
        "kalnoy/nestedset": "^5.0",
        "codex-team/editor.js": "^2.0",
        "proengsoft/laravel-jsvalidation": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Stankiewiczpl\\LaravelForms\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stankiewiczpl\\LaravelForms\\LaravelFormsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
